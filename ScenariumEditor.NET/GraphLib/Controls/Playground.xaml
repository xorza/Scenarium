<UserControl x:Class="GraphLib.Controls.Playground"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:GraphLib.Controls"
             mc:Ignorable="d"
             d:DesignHeight="150" d:DesignWidth="150"
             Foreground="{StaticResource DarkWhite}"
             Background="{x:Null}">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles/TabControll.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>


        </ResourceDictionary>
    </UserControl.Resources>

    <TabControl Style="{StaticResource TabControlDefault}">
        <TabItem>
            <TabItem.Header>
                <TextBlock FontWeight="Bold">Properties</TextBlock>
            </TabItem.Header>

        </TabItem>
        <TabItem IsSelected="True">
            <TabItem.Header>
                <TextBlock FontWeight="Bold">Collection</TextBlock>
            </TabItem.Header>

        </TabItem>
    </TabControl>

</UserControl>